<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.TbPlanLogMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pojo.TbPlanLog">
        <id column="plan_log_id" property="planLogId" />
        <result column="plan_id" property="planId" />
        <result column="process" property="process" />
        <result column="plan_log_content" property="planLogContent" />
        <result column="log_time" property="logTime" />
        <result column="create_at" property="createAt" />
        <result column="update_at" property="updateAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        plan_log_id, plan_id, process, plan_log_content, log_time, create_at, update_at
    </sql>

</mapper>
